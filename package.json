{
  "name": "nookure-frontend",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_ENV=production cp app/entry.server.tsx.ignored app/entry.server.tsx && react-router build ; rm -r  app/entry.server.tsx",
    "dev": "NODE_ENV=development react-router dev",
    "start": "cross-env NODE_ENV=production react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "cloudflare:dev": "wrangler dev"
  },
  "dependencies": {
    "@nextui-org/react": "^2.6.10",
    "@react-router/node": "^7.1.1",
    "@react-router/serve": "^7.1.1",
    "framer-motion": "^11.15.0",
    "isbot": "^5.1.17",
    "mitt": "^3.0.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.1.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241112.0",
    "@hiogawa/vite-node-miniflare": "0.1.1",
    "@react-router/dev": "^7.1.1",
    "@types/node": "^20",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.16",
    "tailwindcss-view-transitions": "^0.1.1",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
